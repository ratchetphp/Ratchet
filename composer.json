        "react/event-loop": ">=0.4",
        "guzzlehttp/psr7": "^1.7|^2.0",
        "symfony/http-foundation": "^2.6|^3.0|^4.0|^5.0|^6.0",
        "symfony/routing": "^2.6|^3.0|^4.0|^5.0|^6.0"
