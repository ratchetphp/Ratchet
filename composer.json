        "Server",
        "Ratchet",
        "Sockets",
        "WebSocket"
    ],
    "homepage": "http://socketo.me",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Boden",
            "email": "cboden@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Matt Bonneau",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/ratchetphp/Ratchet/issues",
